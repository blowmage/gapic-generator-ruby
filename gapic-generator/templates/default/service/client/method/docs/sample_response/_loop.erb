<%- assert_locals values -%>
<%= values["collection"].gsub("$resp", "response") %>.each do |result|
  <%- values["body"].each do |b| -%>
  <%- if b.key? "comment" -%>
  <%= indent b["comment"].join("\n"), "# " %>
  <%- end -%>
  <%- if b.key? "define" -%>
  <%= b["define"] %>
  <%- end -%>
  <%- if b.key? "print" -%>
  puts "<%= b["print"][0].gsub("%s", "\#{%s}") % b["print"][1..-1] %>"
  <%- end -%>
  <%- end -%>
end
